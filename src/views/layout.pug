// Page Wrapper
#wrapper

  // Sidebar
  ul#accordionSidebar.navbar-nav.bg-gradient-primary.sidebar.sidebar-dark.accordion

    // Sidebar - Brand
    a.sidebar-brand.d-flex.align-items-center.justify-content-center(href='?page=index')
      .sidebar-brand-icon.rotate-n-15
        i.fas.fa-laugh-wink
      .sidebar-brand-text.mx-3 Buzzcut

    // Divider
    hr.sidebar-divider.my-0

    // Nav Item - Dashboard
    li.nav-item.active
      a.nav-link(href='/object-detection')
        i.fas.fa-fw.fa-tachometer-alt
        span Object Detection

    // Divider
    hr.sidebar-divider.d-none.d-md-block

    // Sidebar Toggler (Sidebar)
    .text-center.d-none.d-md-inline
      button#sidebarToggle.rounded-circle.border-0
  // End of Sidebar

  // Content Wrapper
  #content-wrapper.d-flex.flex-column

    // Main Content
    #content

      // Topbar
      nav.navbar.navbar-expand.navbar-light.bg-white.topbar.mb-4.static-top.shadow
        // Sidebar Toggle (Topbar)
        button#sidebarToggleTop.btn.btn-link.d-md-none.rounded-circle.mr-3
          i.fa.fa-bars
        // Page Heading
        form.d-sm-inline-block.form-inline.mr-auto.ml-md-3.my-2.my-md-0.mw-100
          h1.h3.mb-0.text-gray-800 Object Detection
      // End of Topbar

    
      // Begin Page Content
      block content

    // End of Main Content

    // End of Footer
  // End of Content Wrapper

